{
    "id": "prompt_engineer_module_a1b2",
    "name": "Prompt Engineer",
    "version": "2.0",
    "icon_file": "icon.png",
    "author": "AWENK AUDICO",
    "email": "contributor@teetah.art",
    "website": "https://www.teetah.art",
    "description": "Selects a pre-saved prompt template from the Prompt Manager and outputs its content. (Refactored to align with architecture)",
    "type": "ACTION",
    "entry_point": "processor.PromptEngineerModule",
    "tier": "free",
    "behaviors": [
        "retry",
        "loop"
    ],
    "requires_services": [
        "logger",
        "loc",
        "kernel",
        "prompt_manager_service"
    ],
    "properties": [
        {
            "id": "selected_prompt_id",
            "type": "enum",
            "label": "loc.prop_prompt_template_select_label",
            "default": ""
        }
    ],
    "output_ports": [
        {
            "name": "success",
            "display_name": "success"
        },
        {
            "name": "error",
            "display_name": "error"
        }
    ],
    "output_schema": [
        {
            "name": "data.prompt_template",
            "type": "string",
            "description": "The full text content of the selected prompt template."
        }
    ]
}